        <Button x:Name="start" Content="Начать" HorizontalAlignment="Left" Height="81" Margin="689,38,0,0" VerticalAlignment="Top" Width="419" RenderTransformOrigin="0.5,0.5" FontSize="48" Background="#FFFFDCDC" FontFamily="Cascadia Code Light" Click="start_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="3.245"/>
                    <RotateTransform Angle="-0.107"/>
                    <TranslateTransform X="2.977" Y="-0.006"/>
                </TransformGroup>
